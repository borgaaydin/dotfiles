"*":
  core:
    autoHideMenuBar: true
    disabledPackages: [
      "metrics"
      "exception-reporting"
    ]
    telemetryConsent: "no"
  editor:
    showIndentGuide: true
    tabLength: 4
  "linter-flake8":
    ignoreErrorCodes: [
      "E501"
    ]
  "package-sync": {}
  welcome:
    showOnStartup: false
